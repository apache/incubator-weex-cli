{
  "name": "weex-toolkit",
  "version": "2.0.0-beta.31",
  "description": "Weex CLI Toolkit",
  "bin": {
    "weex": "bin/weex.js"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint --fix lib/*.js bin/*.js",
    "publish": "npm run test && npm run lint && lerna publish",
    "init": "lerna bootstrap"
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apache/weex-cli.git"
  },
  "author": "Mario <faterrole@gamil.com>",
  "license": "MIT",
  "dependencies": {
    "colors": "^1.3.0",
    "cross-spawn": "^6.0.5",
    "debug": "^3.1.0",
    "fs-extra": "^6.0.1",
    "got": "^8.3.1",
    "inquirer": "^6.2.2",
    "npminstall": "^3.8.0",
    "on-exit": "^1.0.1",
    "root-check": "^1.0.0",
    "semver": "^5.5.0",
    "source-map": "^0.7.3",
    "tabtab": "^2.2.2",
    "userhome": "^1.0.0",
    "yargs-parser": "^10.1.0"
  },
  "devDependencies": {
    "eslint": "^5.11.1",
    "eslint-plugin-node": "^8.0.0",
    "jest": "^23.6.0",
    "lerna": "^3.8.4",
    "lint-staged": "^8.1.0",
    "np": "^3.1.0",
    "sinon": "^7.2.2",
    "ts-node": "^6.1.0",
    "typescript": "^2.9.1",
    "typescript-plugin-add-type": "^1.0.4",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommitMessage.js"
  },
  "lint-staged": {
    "lib/*.{js}": [
      "eslint --fix",
      "git add"
    ],
    "bin/*.{js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/scripts/testSetup.js"
    ],
    "testMatch": [
      "<rootDir>/lib/*.spec.js"
    ]
  }
}
